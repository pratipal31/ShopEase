# Copy to .env and fill values
MONGO_URI=your_mongo_connection_string_here
PORT=5000
CLIENT_URL=http://localhost:5173

# JWT secrets
JWT_SECRET=replace_with_a_strong_secret
JWT_REFRESH_SECRET=replace_with_a_strong_refresh_secret

# Admin seeding and registration protection
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=ChangeMeToAStrongPassword
# Optional: ADMIN_SECRET can be used to allow creating admin via /api/auth/register
ADMIN_SECRET=some_admin_secret_value
